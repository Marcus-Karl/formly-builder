{
  "name": "formly-builder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "clean": "",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "stats": "ng build --stats-json && webpack-bundle-analyzer dist/formly-builder/stats.json",
    "i18n": "npm run i18n:default && npm run i18n:extract",
    "i18n:default": "ngx-translate-extract --input ./src --output ./src/assets/i18n/system-en.json --key-as-default-value --clean --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/es.json --clean --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "^12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/material": "^12.1.3",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-formly/core": "^5.10.33",
    "@ngx-formly/material": "^5.10.33",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "^6.0.0",
    "json-rules-engine": "^6.1.2",
    "jsonpath-plus": "^6.0.1",
    "lodash": "^4.17.21",
    "ngx-editor": "^11.1.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/jasmine": "~3.8.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.2",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
