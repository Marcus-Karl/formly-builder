{
  "name": "formly-builder",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "build": "ng build",
    "clean": "",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "stats": "ng build --stats-json && webpack-bundle-analyzer dist/formly-builder/stats.json",
    "i18n": "npm run i18n:default && npm run i18n:extract",
    "i18n:default": "ngx-translate-extract --input ./src --output ./src/assets/i18n/system-en.json --key-as-default-value --clean --format json",
    "i18n:extract": "ngx-translate-extract --input ./src --output ./src/assets/i18n/en.json ./src/assets/i18n/es.json --clean --format json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.1",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^13.3.1",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@ngx-formly/core": "^5.11.2",
    "@ngx-formly/material": "^5.11.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@stoplight/json-ref-resolver": "^3.1.3",
    "json-rules-engine": "^6.1.2",
    "jsonpath-plus": "^6.0.1",
    "ngx-editor": "^12.2.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@biesbjerg/ngx-translate-extract": "^7.0.4",
    "@types/jasmine": "~4.0.0",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "jasmine-core": "~4.0.1",
    "karma": "~6.3.17",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.3",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}